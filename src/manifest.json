{
  "name": "yajamon-content-scripts-pack",
  "version": "1.0.0",
  "description": "Content Scripts package. for Chrome Extension.",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "*://bitbucket.org/*/pull-requests/new",
        "*://bitbucket.org/*/pull-requests/update/*"
      ],
      "js": ["js/bitbucket.org/new-pull-request.js"]
    }
  ]
}
